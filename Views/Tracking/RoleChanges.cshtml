@model List<RoleChange>
<div class="text-center">
    <h1>Role Changes</h1>
</div>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Type</th>
            <th>Affected User</th>
            <th>Role</th>
            <th>Time of Change</th>
            <th>Admin</th>

        </tr>

    </thead>
    <tbody>
        @foreach (RoleChange rc in Model)
        {
            <tr>
                <td>@rc.Type</td>
                <td>@rc.UserAffected.UserName</td>
                <td>@rc.RoleAffected</td>
                <td>@rc.TimeOfChange</td>
                <td>@rc.Admin.UserName</td>
            </tr>
        }

    </tbody>
</table>